{
  "name": "test",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "lint": "eslint src/**/*.{ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx,json}",
    "format:fix": "prettier --write src/**/*.{ts,tsx,css,scss,json}",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint",
      "npm run format:fix"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "xml-js": "^1.6.11",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0"
  }
}
